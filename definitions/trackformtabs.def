card: {"title":"Tracking","icon-name":"custom:custom14"}
    layout: {"multiple-rows":"readonly"}
        item: {"size": 12, "padding":"around-small", "small-device-size":"12"}
            layout:
                item: {"size":6, "padding":"around-small"}
                    input: {"label":"Tracking Number", "value":"{trackingNumber}"}
            layout:
                item: {"size":6, "padding":"around-small"}
                    button: {"label":"Search", "onclick":"{inquireTrack}"}
        tabset:
            tab: {"label": "Status"}
                item: {"size": 12, "padding":"around-small", "small-device-size":"12"}
                    layout:
                        item: {"size":6, "padding":"horizontal-small"}
                            input: {"label":"Tracking Number", "value":"{trackingNumber}", "readonly":null}
                        item: {"size":6, "padding":"horizontal-small"}
                            input: {"label":"Tracking Status", "value":"{trackingStatus}", "readonly":null}
                    layout:
                        item: {"size":6, "padding":"horizontal-small"}
                            input: {"label":"Service", "value":"{serviceType}", "readonly":null}
                        item: {"size":6, "padding":"horizontal-small"}
                            input: {"label":"Packaging", "value":"{packaging}", "readonly":null}
                    layout:
                        item: {"size":6, "padding":"horizontal-small"}
                            input: {"label":"Dimensions", "value":"{packageDimensions}", "readonly":null}
                        item: {"size":6, "padding":"horizontal-small"}
                            input: {"label":"Weight", "value":"{packageWeight}", "readonly":null}
            tab: {"label": "Parties"}
                item: {"size": 12, "padding":"around-small", "small-device-size":"12"}
                    layout: 
                        item: {"size": 6, "medium-device-size": 6}
                            layout:
                                item: {"size": 12, "padding":"horizontal-small"} 
                                    input: {"label":"Shipper Company", "value":"{shipperCompany}", "readonly":null}
                                    input: {"label":"Name", "value":"{shipperName}", "readonly":null}
                                    input: {"label":"Address", "value":"{shipperAddress1}", "readonly":null}
                                    input: {"label":" ", "value":"{shipperAddress2}", "readonly":null}
                            layout:
                                item: {"size": 5, "padding":"horizontal-small"} 
                                    input: {"label":"City", "value":"{shipperCity}", "readonly":null}
                                item: {"size": 2, "padding":"horizontal-small"} 
                                    input: {"label":"State", "value":"{shipperState}", "readonly":null}
                                item: {"size": 2, "padding":"horizontal-small"} 
                                    input: {"label":"Country", "value":"{shipperCountry}", "readonly":null}
                                item: {"size": 3, "padding":"horizontal-small"} 
                                    input: {"label":"Zip", "value":"{shipperZip}", "readonly":null}
                        item: {"size": 6, "medium-device-size": 6}  
                            layout:
                                item: {"size": 12, "padding":"horizontal-small"} 
                                    input: {"label":"Recipient Company", "value":"{recipientCompany}", "readonly":null}
                                    input: {"label":"Name", "value":"{shipperName}", "readonly":null}
                                    input: {"label":"Address", "value":"{recipientAddress1}", "readonly":null}
                                    input: {"label":" ", "value":"{recipientAddress2}", "readonly":null}
                            layout:
                                item: {"size": 5, "padding":"horizontal-small"} 
                                    input: {"label":"City", "value":"{recipientCity}", "readonly":null}
                                item: {"size": 2, "padding":"horizontal-small"} 
                                    input: {"label":"State", "value":"{recipientState}", "readonly":null}
                                item: {"size": 2, "padding":"horizontal-small"} 
                                    input: {"label":"Country", "value":"{recipientCountry}", "readonly":null}
                                item: {"size": 3, "padding":"horizontal-small"} 
                                    input: {"label":"Zip", "value":"{recipientZip}", "readonly":null}
            tab: {"label": "Scans"}
                item: {"size": 12, "padding":"around-small", "small-device-size":"12"}
                    layout:
                        item: {"size":3, "padding":"horizontal-small"}
                            input: {"label":"Scan Time", "value":"{scanTime}", "readonly":null}
                        item: {"size":2, "padding":"horizontal-small"}
                            input: {"label":"Code", "value":"{scanCode}", "readonly":null}
                        item: {"size":3, "padding":"horizontal-small"}
                            input: {"label":"Status", "value":"{scanStatus}", "readonly":null}
                        item: {"size":4, "padding":"horizontal-small"}
                            input: {"label":"Location", "value":"{scanLocation}", "readonly":null}
